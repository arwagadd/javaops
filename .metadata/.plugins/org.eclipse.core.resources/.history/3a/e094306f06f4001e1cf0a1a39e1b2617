package javaoperations;

public class Student extends Person {

	@Override
	public String getPosition() {
		return getPosition();
	}
	
	public static void main(String[] args) {
		Student s1 = new Student();
		s1.setName("Arwa");
		s1.setAge(22);
		s1.setGender(Gender.FEMALE);
		s1.setPosition("Student");
	}

}
