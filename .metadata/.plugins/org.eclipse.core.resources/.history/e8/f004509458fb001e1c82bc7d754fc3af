package javaoperations;

public class StringBufferExample {
	
	public static StringBuffer  appendToString(StringBuffer s,String str) {
		return s.append(str);
	}
	public static void main(String[] args) {
		StringBuffer str = new StringBuffer("Arwa");
		System.out.println(appendToString(str,"Wail"));
	}
	
}
